namespace ca.mcgill.ecse223.resto.model;

class Table{
  state {
    tableState_notInUse {
      setTableState_inUse -> tableState_inUse;
      tableState_available {
        setTableState_reserved -> tableState_reserved;
      }
      tableState_reserved {
      setTableState_cancel -> tableState_available;
      }
    }
    tableState_inUse {
      setTableState_order -> tableState_inUse;
      setTableState_issueBill -> tableState_available;
    }
  }
}